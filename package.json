{
  "name": "socket.io-file",
  "version": "3.0.0",
  "description": "File uploader module for Socket.io",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run build-prod;npm run serve",
    "build-dev": "NODE_ENV=development babel ./src -d ./build --source-maps",
    "build-prod": "NODE_ENV=production babel ./src -d ./build --presets minify",
    "serve": "node ./build/index.js",
    "flow": "flow",
    "dev": "npm run flow;npm run build-dev;nodemon --watch src/ --exec \"npm run start\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rico345100/socket.io-file"
  },
  "keywords": [
    "WebSocket",
    "Socket.io",
    "binary",
    "arraybuffer",
    "file",
    "upload",
    "node.js"
  ],
  "author": ".modernator",
  "license": "MIT",
  "dependencies": {
    "mime": "^1.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-flow": "^7.0.0",
    "babel-preset-minify": "^0.5.0",
    "flow-bin": "^0.95.1",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10"
  }
}
